package 多线程;

/*
 * 需求：两储户，存钱每次存100，共存3次
 * */

class Bank{
	private int sum;
	public void add(int num){
		sum+=num;
		System.out.println("sum=" + sum);
	}
}

class Cus implements Runnable{
	public void run(){
		Bank b = new Bank();
		for(int x=0;x<3;x++){
			b.add(100);
		}
	}
}

public class BankDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

	}

}
