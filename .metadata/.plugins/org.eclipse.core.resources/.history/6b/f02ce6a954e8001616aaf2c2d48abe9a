package ผฬณะ;

class Phone{
	
	void show(){
		System.out.println("number");
		
	}
}
	class NewPhone extends Phone{
		void newShow(){
			System.out.println("name");
		}
	}

	public class ExtendsDemo3 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Phone p = new Phone();
		p.show();
	}
}
