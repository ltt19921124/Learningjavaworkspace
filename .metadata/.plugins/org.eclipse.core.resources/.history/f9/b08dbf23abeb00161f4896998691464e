package ∂‡œﬂ≥ÃMars;

public class Runnable_1 implements Runnable{

	/**
	 * @param args
	 */
	public void run(){
		for(int i=0;i < 100;i++){
			System.out.println("Runnable-->" + i);
		}
	}

}

class Runnable_2{
	
	public static void main(String[] args){
		Runnable_1 ri = new Runnable_1();
		Thread t = new Thread(ri);
		t.start();
		for(int i=0;i < 100;i ++){
			System.out.println("main-->" + i);
		}
	}
}