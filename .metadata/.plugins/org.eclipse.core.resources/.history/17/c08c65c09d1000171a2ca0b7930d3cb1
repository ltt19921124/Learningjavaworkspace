package join;

public class TestYield extends Thread{
	
	public TestYield(){
		
	}
	public TestYield(String name){
		super(name);
	}
	public void run(){
		for(int i=0;i < 50;i++){
			System.out.println(getName() + "..." + i);
		}
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {

	}

}
