package Sort;

public class Sort {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] arr = {3,7,89,90,123,56};
		insertsort(arr);
	}

	private static void insertsort(int[] arr) {
		for (int i = 1; i < arr.length; i++) {
			int temp = arr[i];
			int j = i - 1;
			for (; j >= 0 && temp < arr[j]; j--) {
				arr[j+1] = arr[j];
			}
			arr[j+1] = temp;
		}
		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
	}

		
}
