package day10;

class Fu{
	int num = 3;
	void show(){
		System.out.println("fu show");
	}
	static void method(){
		System.out.println("fu static method");
	}
}

class Zi extends Fu{
	int num = 4;
	void show(){
		System.out.println("zi show");
	}
	static void show(){
		System.out.println("zi static method");
	}
}

public class DuotaiDemo3 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

	}

}
