package day09;


abstract class Employee{
	private String name;
	
	private String id;
	private double pay;
	Employee(String name,String id,double pay){
		this.name = name;
		this.id = id;
		this.pay = pay;
	}
	public abstract void work();
}

class Manager extends Employee{

	Manager(String name, String id, double pay) {
		super(name, id, pay);
		// TODO Auto-generated constructor stub
	}
	
}

