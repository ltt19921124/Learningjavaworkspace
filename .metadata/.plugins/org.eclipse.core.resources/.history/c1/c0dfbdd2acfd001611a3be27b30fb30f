package 第四章流程控制和数组;

public class TestReferenceArray {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Person[] students;
		students = new Person[2];
		
		Person zhang = new Person();
		zhang.age = 15;
		zhang.height = 158;
		
		Person lee = new Person();
		lee.age = 16;
		lee.height = 161;
		
		
	}

}
