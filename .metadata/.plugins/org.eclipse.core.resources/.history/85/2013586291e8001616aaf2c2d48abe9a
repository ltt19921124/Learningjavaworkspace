/*
 * 抽象类
 * 抽象：笼统，模糊
 * 
 * */

package 抽象类;

class Demo{
	void show(){
		System.out.println("demo show");
	}
}

class DemoA{
	void show(){
		System.out.println("demoa");
	}
}

class DemoB{
	void show(){
		System.out.println("demob");
	}
}

public class AbstractDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

	}

}
