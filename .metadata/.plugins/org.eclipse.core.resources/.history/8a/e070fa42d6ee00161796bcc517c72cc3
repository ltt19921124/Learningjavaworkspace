package 集合框架;

import java.util.ArrayList;
import java.util.List;

public class ListDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		List list = new ArrayList();
		show(list);
		
		
	}
	public static void show(List list){
		//添加元素
		list.add("abc1");
		list.add("abc2");
		list.add("abc3");
		list.add("abc4");
		//插入元素
		System.out.println(list);
		
		list.add(1,"abc9");
		System.out.println(list);
	}
	
	
}
