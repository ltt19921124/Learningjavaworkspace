
public class Test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		/*一维数组：int []arr = new int[3];
		 * 二维数组
		 * 
		 * 
		 * */
		int[][]arr = new int[3][2];//创建一个二维数组，该数组中有3个一维数组，每个
		//一维数组有两个元素。
		System.out.println(arr);//直接打印二维数组，
//		System.out.println(arr[0]);//直接打印二维数组中的角标0的一维数组
		
	}

}
