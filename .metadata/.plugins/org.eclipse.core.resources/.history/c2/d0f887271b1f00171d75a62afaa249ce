package aa;

public class ArrayDemo4 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] arr = {4,5,6,23,12,78};
		
		
	}
	
	public static int getMax(int[] arr){
		int max = arr[0];
		for(int x=0;x<arr.length;x++){
			if(arr[x] > max){
				max = arr[x];
			}
		}
		return max;
	}
}
