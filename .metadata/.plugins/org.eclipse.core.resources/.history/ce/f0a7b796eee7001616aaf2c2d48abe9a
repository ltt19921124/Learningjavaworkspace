package 继承;

/*
 * 
 * 在子父类中，成员的特点
 * 1，成员变量
 * 2，成员函数
 * 3，构造函数
 * 
 * */
//1，成员变量
class Fu{
	int num1 = 4;
}

class Zi extends Fu{
	int num2 = 5;
	void show(){
		System.out.println(num2+"..."+num1);
	}
}

public class ExtendsDemo2 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Zi z = new Zi();
		z.show();
	}

}
