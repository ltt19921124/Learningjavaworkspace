		 * 
		 * 		1.2根据为主获取字符：char (int index)
		 * 		
		 * 
		 * 
		 * 
		 * 
		 * */
		stringMethodDemo_1();
		
	}

	private static void stringMethodDemo_1() {
		String s = "abcd";
		
	}

