package 第五章面向对象;

public class Overload {

	/**
	 * @param args
	 */
	public void test(){
		System.out.println("无参数");
	}
	public void test(String msg){
		System.out.println("重载的test方法" + msg);
	}
	public static void main(String[] args) {
		Overload ol = new Overload();
		ol.test();
		ol.test("hello");
	}

}
