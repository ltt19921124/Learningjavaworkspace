package 设计模式;

class Single3{//类加载进来没有对象，只有调用了getInstance方法，才会创建对象。
	private static Single3 s1 = null;
	private Single3(){
		
	}
	public static Single getInstance1(){
		if(s1 == null)
			s1 = new Single3();
		return s1;
	}
}

