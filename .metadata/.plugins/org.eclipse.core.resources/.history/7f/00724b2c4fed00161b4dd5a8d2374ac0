package string类;

public class StringConstructorDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		StringContructorDemo();
		
		byte[] arr = {65,66,67,68};
		
	}

	private static void StringContructorDemo() {
		String s = new String();//等效于string s = "";不等效于String s = null;
		
		
	}

}
