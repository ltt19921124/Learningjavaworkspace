package day04;

public class FunctionDemo4 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		int i = add(5,8,9);
		System.out.println(add(5,7,8));
	}

	public static int add(int i, int j, int k) {
		
		return i + j + k;
		
	}
	
	public static double add(double a,double b){
		return a + b;
	}

}
