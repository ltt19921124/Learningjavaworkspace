package duoxiancheng;

public class TestEquals {

	public static void main(String[] args){
		Cat c1 = new Cat();
		Cat c2 = new Cat();
		
		System.out.println(c1.equals(c2));
	}
}

class Cat{
	String color;
	int height,weight;
	public Cat(String color,int height,int weight){
		this.color = color;
		this.height = height;
		this.weight = weight;
	}
}