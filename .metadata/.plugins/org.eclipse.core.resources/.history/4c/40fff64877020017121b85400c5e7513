package ∂‡œﬂ≥ÃMars;

public class RunnableImpl implements Runnable{

	public void run(){
		for (int i = 0; i < 10; i++) {
			System.out.println("Runnable-->" + i);
			if(i == 5){
				try {
					Thread.sleep(2000);
				} catch (InterruptedException e) {
					e.printStackTrace();
				}
			}
		}
	}


}
