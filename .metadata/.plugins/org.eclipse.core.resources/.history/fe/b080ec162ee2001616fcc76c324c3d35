public class ArrayDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		/*
		 * 
		 * */
		// 位置置换
		int[] arr = { 4, 7,7, 8, 9, 45, 20 };
		int index = getMax(arr, 7);
		System.out.println("index=" + index);
		//数组常见功能：查找
	}

	public static int getMax(int[] arr, int key) {
		for (int x = 0; x < arr.length; x++) {
			if (arr[x] == key) {
				return x;
			}
		}
		return -1;
	}
	/*
	 * 二分查找
	 * */
	
	
	
	
	
	
}
