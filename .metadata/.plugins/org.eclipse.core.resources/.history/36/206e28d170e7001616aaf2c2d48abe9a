
class Person{
	private String name;
	private int age;
	Person(){
		name = "baby";
		age = 1;
		System.out.println("person run");
	}
	Person(String name){
		this.name = name;
	}
	
	Person(String n,int a){
		name = n;
		age = a;
	}
	public void speak(){
		System.out.println(name+":" + age);
	}
}

public class ThisDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Person p = new Person();
	}

}
