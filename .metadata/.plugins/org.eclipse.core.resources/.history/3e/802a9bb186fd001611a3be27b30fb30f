package 第四章流程控制和数组;

public class TestIfError {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int age = 45;
		
		if(age<0 || age>120){
			System.out.println("输入的年龄为假");
		}
		
		if(age>20 && age<40){
			System.out.println("青年人");
		}
		else if(age>=40 && age<60){
			System.out.println("中年人");
		}
		else if(age>=60){
			System.out.println("老年人");
		}
	}

}
