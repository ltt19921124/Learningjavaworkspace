package duoxiancheng;

class Demo{
	
}

class ThreadDemo {

	/**
	 * @param args
	 * 多线程优势：解决了多部分代码同时运行的问题
	 * 多线程的弊端：线程太多会导致效率的降低
	 * cpu切换速度特别快
	 * 垃圾回收线程
	 * 主线程
	 * JVM启动时就启动了多个线程，至少有两个线程可以分析出来
	 * 1，执行main函数的线程
	 * 2，负责垃圾回收的线程
	 */
	public static void main(String[] args) {
		new Demo();
		new Demo();
		new Demo();
		System.out.println("ok");
	}

}
