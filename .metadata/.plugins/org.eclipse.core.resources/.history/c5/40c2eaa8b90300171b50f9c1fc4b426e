package 线程通信;

class Resource{
	String name;
	String sex;
}
//输入
class Input implements Runnable{
	public void run(){
		
	}
}

//输出
class Output implements Runnable{
	public void run(){
		while(true){
			System.out.println(name+"..."+sex);
		}
	}
}

public class ResourceDemo {

	/**
	 * @param args
	 * 线程间通信：
	 * 多个线程在处理同一资源，但是任务不同
	 * 
	 */
	public static void main(String[] args) {

	}

}
