package day08;

class ExtendsDemo {

	String name;
	int age;
}

class Student extends ExtendsDemo{
	
	String name;
	int age;
	
	void study(){
		System.out.println(name + "...study" + age);
	}
}
class Worker extends ExtendsDemo
{
//	String name;
//	int age;
	void work()
	{
		System.out.println("worker work");
	}
}

class ExtendsDemo 
{
	public static void main(String[] args) 
	{
		Student s = new Student();
		s.name= "zhangsan";
		s.age = 22;
		s.study();
	}
}




