

class Person{
	String name;
	String country = "CN";
	public void show(){
		System.out.println(country+":" + name);
	}
}

public class StaticDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Person p  = new Person(); 
//		p.name = "小强";
//		p.show();//CN:小强
		System.out.println(p.country);
		System.out.println(Person.country);
		
	}

}
