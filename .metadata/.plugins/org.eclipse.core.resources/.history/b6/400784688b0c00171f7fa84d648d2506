package duoxiancheng;

class Demo{
	private String name;
	Demo(String name){
		this.name = name;
	}
	public void show(){
		for (int i = 0; i < 10; i++) {
			System.out.println(name + "...i=" + i);
		}
	}
}

public class ThreadDemo2 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Demo d1 = new Demo("zhangsan");
		Demo d2 = new Demo("ÀîËÄ");
		d1.show();
		d2.show();
		
	}

}
