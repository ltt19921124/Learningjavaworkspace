package aa;

public class ArrayDemo5 {

	/**
	 * @param args
	 * 数组常见功能：查找
	 */
	public static void main(String[] args) {
		int[]arr = {1,2,34,5,7};
//		System.out.println(getIndex(arr,3));//-1
		System.out.println(getIndex(arr,7));//1
		
	}
	//查找
	public static int getIndex(int[] arr,int key){
		for (int i = 0; i < arr.length; i++) {
			if(arr[i] == key){
				return i;
			}
		}
		return -1;
	}

}
