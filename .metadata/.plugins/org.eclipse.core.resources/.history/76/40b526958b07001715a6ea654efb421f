package day05;

public class ArrayDemo4 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] arr = {78,67,34,56,90};
		printArray(arr);
	}

	private static void printArray(int[] arr) {
		System.out.print("[");
		for (int i = 0; i < arr.length; i++) {
			if (i != arr.length - 1){
				System.out.print(arr[i] + ",");
			}
			System.out.println(arr[i] + "]");
		}
	}
	
	

}
