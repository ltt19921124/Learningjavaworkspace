package 集合框架;

import java.util.ArrayList;
import java.util.Collection;

public class IteratorDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Collection coll = new ArrayList();//
		
		coll.add("abc1");
		coll.add("abc2");
		coll.add("abc3");
		coll.add("abc4");
		
		//使用了Collection中的iterator()方法,调用集合中的迭代器方法是为了huode
		//iterator对象
		coll.iterator();
	}

}
